  "mapType": "Theon",
  "dialogues": [
    {
      "speaker": "Başanalist Kelvion",
      "text": "Kutsal rüyalarını test etmek istiyoruz. Uyurken seni izleyebilir miyiz?",
      "options": [
        {
          "text": "Hayır, Tanrı’nın sözü gizlidir",
          "effects": {
            "faith": 7,
            "trust": -5,
            "hostility": 2
          }
        },
        {
          "text": "Sadece bir gece",
          "effects": {
            "faith": 2,
            "trust": 3,
            "hostility": 0
          }
        },
        {
          "text": "Tamam, ne gerekiyorsa yapın",
          "effects": {
            "faith": -3,
            "trust": 6,
            "hostility": 0
          }
        }
      ],
      "id": "theon_01",
      "type": "special"
    },
    {

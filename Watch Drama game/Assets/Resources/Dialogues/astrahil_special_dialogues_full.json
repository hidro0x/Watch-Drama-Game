    {
      "id": "astrahil_01",
      "type": "special",
      "speaker": "Bilgelik Gözcüsü A",
      "text": "Kehanetin arşivlerimizle çelişiyor. Bu farkı nasıl açıklarsın?",
      "options": [
        {
          "text": "Arşivler geçmişi yazar, ben geleceği.",
          "effects": {
            "faith": 6,
            "trust": 4,
            "hostility": 0
          }
        },
        {
          "text": "Kehanet yoruma açıktır, tıpkı tarih gibi.",
          "effects": {
            "faith": 4,
            "trust": 3,
            "hostility": 1
          }
        },
        {
          "text": "Hiçbir şeyi açıklamak zorunda değilim.",
          "effects": {
            "faith": 1,
            "trust": -2,
            "hostility": 3
          }
        }
      ]
    },
